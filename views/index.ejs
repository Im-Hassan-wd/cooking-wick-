<div class="content">
    <h2>All Recipes</h2>

    <% if (recipes.length > 0) { %>
      <% recipes.forEach(recipe => { %>
        <div class="recipe-preview">
            <h3><%= recipe.title %></h3>
            <p><%= recipe.ingrident %></p>
        </div>
      <% }) %>
    <% } %>
</div>

